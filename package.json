    "s3",
    "upload",
    "vanillajs",
    "vue-aws3",
    "vuejs",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "short-uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "microbundle": "^0.9.0",
    "typescript": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fausto95/aws-s3"
  },
  "bugs": {
    "url": "https://github.com/Fausto95/aws-s3/issues"
  }
