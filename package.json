    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "cjs",
    "esm",
    "umd"
  ],
  "keywords": [
    "angular-aws3",
    "angular",
    "aws",
    "file-upload",
    "file",
    "react-aws3",
    "react-component",
